
#tags
    display:block
    margin-left:auto
    margin-right:auto
    margin-top:1em
    margin-bottom:1em
    width:50%
    clear:both

.tag
    margin:10px
    float:left
    font-weight:bold
    font-size:16
    text-decoration:none
    display:block

    color:#3b5d14
    background:#cceecc
    border-radius:5px
    padding:10px

.tag:hover
    background:#bbddbb

